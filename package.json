{
  "name": "femiadeniyi",
  "version": "0.3.4",
  "private": true,
  "scripts": {
    "packages:publish": "lerna publish --ignore-changes testing-tools cra-template-femi --conventional-commits",
    "packages:scripts": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} LERNA_ROOT_PATH=C:\\Software\\femiadeniyi lerna exec --parallel -- ts-node $LERNA_ROOT_PATH/scripts/index.ts",
    "packages:tsc": "lerna execd --ignore cra-template-femi -- tsc",
    "packages:publish-setup": "lerna exec -- npm publish --access public",
    "packages:clean": "lerna exec del-cli dist",
    "prepublish": "concurrently \"terraform init && terraform apply -auto-approve\" \"npm run packages:scripts\" && npm run packages:tsc",
    "release": "standard-version --dry-run"
  },

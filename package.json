    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.6",
    "bootstrap": "^4.6.0",
    "change-case": "^4.1.2",
    "chrome-launcher": "^0.13.4",
    "commander": "^7.0.0",
    "commitizen": "^4.2.3",
    "concurrently": "^5.3.0",
